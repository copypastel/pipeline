<script>
  <% unless @video.nil? %>
  document.title = "Pipeline :: " + "<%= escape_html(@video.title) %>";

  function onYouTubePlayerReady(){
    var video = document.getElementById("video_player");
    video.loadVideoById("<%= @video.video_id %>", 0);
    video.playVideo();
  }
  <% end %>
</script>